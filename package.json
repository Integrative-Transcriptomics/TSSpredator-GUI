{
  "name": "tsspredatorweb",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/pro-light-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@upsetjs/react": "^1.11.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "gosling.js": "^0.16.0",
    "higlass": "^1.13.4",
    "jszip": "^3.10.1",
    "npm-run-all": "^4.1.5",
    "pixi.js": "6.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-spinners": "^0.13.8",
    "react-table": "^7.8.0",
    "vite": "^5.1.6"
  },
  "scripts": {
    "server-redis": "redis-server",
    "server-celery": "celery -A server_tsspredator.celery_app worker -B --loglevel=warning",
    "server": "cd server_tsspredator && flask run --with-threads --debug",
    "server-deploy": "gunicorn --workers 3 --timeout 500 --graceful-timeout 500 --bind 0.0.0.0:3001 --log-level=debug server_tsspredator.wsgi:tsspredator",
    "tsspredator": "run-p server server-redis server-celery start",
    "tsspredator-test": "run-p server-deploy server-redis server-celery",
    "start": "vite",
    "build": "vite build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.1.1"
}
